        <mxCell id="lrS_1NFouU5z3kCtPqEW-6" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;VISTA, TRIGGER, PROCEDURE, FUNCTION&lt;/h1&gt;&lt;p&gt;&lt;b&gt;Vista:&lt;/b&gt;&lt;/p&gt;&lt;p&gt;CREATE VIEW&amp;nbsp; &quot;cultivos actuales&quot; AS&lt;/p&gt;&lt;p&gt;SELECT * FROM&amp;nbsp; cultivos WHERE&lt;/p&gt;&lt;p&gt;fecha_Cosecha IS NULL;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;Trigger:&lt;/b&gt;&lt;/p&gt;&lt;p&gt;DELIMITER//&lt;/p&gt;&lt;p&gt;&amp;nbsp;CREATE TRIGGER evitar_Insercion_Cultivos_Sin_Parcela&amp;nbsp;&lt;/p&gt;&lt;p&gt;BEFORE INSERT ON cultivos&lt;/p&gt;&lt;p&gt;FOR EACH ROW&lt;/p&gt;&lt;p&gt;BEGIN&lt;/p&gt;&lt;p&gt;IF NEW().&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;id_Parcela IS NULL THEN&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;SIGNAL SQLSTATE &#39;45000&#39;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;SET MESSAGE=&#39;No se puede introducir cultivo sin parcela asignada&#39;;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;END IF;&lt;/p&gt;&lt;p&gt;END;&lt;/p&gt;&lt;p&gt;//DELIMITER;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;Procedure:&lt;/b&gt;&lt;/p&gt;&lt;p&gt;DELIMITER//&lt;/p&gt;&lt;p&gt;CREATE PROCEDURE insertar_Nueva_Tarea(&lt;/p&gt;&lt;p&gt;IN&amp;nbsp;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;id_Tarea INT&lt;/span&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;,&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;IN nombre VARCHAR(40), descripcion VARCHAR(400),&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;IN fecha_Inicio DATE,&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;IN fecha_Fin DATE,&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;IN situacion VARCHAR(40)&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;)&lt;/span&gt;&lt;/p&gt;&lt;p&gt;BEGIN&lt;/p&gt;&lt;p&gt;INSERT INTO tareas(&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;id_Tarea, nombre, descripcion, fecha_Inicio, fecha_Fin, situacion&lt;/span&gt;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;)&lt;/span&gt;&lt;/p&gt;&lt;p&gt;VALUES&amp;nbsp;&lt;span style=&quot;background-color: transparent; color: light-dark(rgb(0, 0, 0), rgb(255, 255, 255));&quot;&gt;(1, Recoger maiz, se trata de recoger maiz, 01/02/2025, 10/02/2025, activo);&lt;/span&gt;&lt;/p&gt;&lt;p&gt;END;&lt;/p&gt;&lt;p&gt;//DELIMITER;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
